# LLM Provider Configuration
# Options: anthropic, openai, openrouter, deepseek
LLM_PROVIDER=anthropic

# API Keys (set the one matching your LLM_PROVIDER)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
OPENROUTER_API_KEY=
DEEPSEEK_API_KEY=

# Model to use
# Examples:
#   Anthropic: claude-sonnet-4-20250514, claude-3-5-haiku-20241022
#   OpenAI: gpt-4o, gpt-4o-mini
#   OpenRouter: anthropic/claude-sonnet-4, openai/gpt-4o
#   DeepSeek: deepseek-chat, deepseek-reasoner
LLM_MODEL=claude-sonnet-4-20250514

# Temperature for LLM (0 = deterministic, 1 = creative)
LLM_TEMPERATURE=0

# Seedbox Configuration
# Options: docker, ssh
SEEDBOX_MODE=docker

# Docker settings (used when SEEDBOX_MODE=docker)
DOCKER_CONTAINER_NAME=manus-sandbox

# SSH settings (used when SEEDBOX_MODE=ssh)
SEEDBOX_HOST=
SEEDBOX_PORT=22
SEEDBOX_USER=
SEEDBOX_SSH_KEY_PATH=
SEEDBOX_PASSWORD=

# Execution settings
EXECUTION_TIMEOUT=30
MAX_OUTPUT_LENGTH=5000
MAX_ITERATIONS=30
